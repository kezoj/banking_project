<div class="row justify-content-md-center">
    <h3>Incoming transactions</h3>
</div>
<div class="row justify-content-md-center">
    <ul>
        @foreach (var x in @Model.TransactionModelList)
        {
            <li>Sender Account Id: @x.SenderAccountId</li>
            <li>Amount: @x.Amount</li>
            <li>Status: @x.Status</li>
            <li>Info: @x.Info</li>
            <hr/>
        }
    </ul>
</div>